<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wwx.teamall.mapper.TGoodsMapper">
    <update id="addStock">
        update t_goods set sales = sales + #{amount} where id = #{goodsId}
    </update>
</mapper>
